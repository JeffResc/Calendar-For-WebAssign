{
  "manifest_version": 2,
  "name": "Calendar for WebAssign",
  "version": "1.0",

  "description": "Adds an \"Add to Calendar\" button on WebAssign.",

  "icons": {
    "48": "icons/icons8-calendar-48.png",
    "96": "icons/icons8-calendar-96.png",
    "144": "icons/icons8-calendar-144.png",
    "240": "icons/icons8-calendar-240.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.webassign.net/*"],
      "js": ["ouical.min.js", "main.js"]
    }
  ]
}